{
  "name": "edit-ai",
  "version": "1.0.0",
  "description": "World's First AI-Powered Automated Video Editing System - Transform real footage into multiple professionally edited versions",
  "main": "scripts/bridge-controller.js",
  "scripts": {
    "start": "node scripts/server.js",
    "dev": "nodemon scripts/server.js",
    "test": "node scripts/test-runner.js",
    "setup": "node scripts/setup.js",
    "batch": "node scripts/batch-processor.js",
    "build": "node scripts/build.js",
    "clean": "node scripts/cleanup.js",
    "premiere": "node scripts/premiere-launcher.js",
    "edit": "node scripts/multi-version-editor.js"
  },
  "keywords": [
    "video-editing",
    "automation",
    "premiere-pro",
    "ai",
    "extendscript",
    "adobe",
    "cinematic",
    "vlog",
    "corporate",
    "real-footage",
    "multi-version",
    "edit-ai"
  ],
  "author": "Edit.ai Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/edit-ai.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/edit-ai/issues"
  },
  "homepage": "https://github.com/your-username/edit-ai#readme",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "fs-extra": "^11.1.1",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "inquirer": "^8.2.5",
    "ora": "^5.4.1",
    "node-cron": "^3.0.2",
    "winston": "^3.9.0",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.5.0",
    "eslint": "^8.40.0",
    "prettier": "^2.8.8"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "os": [
    "darwin",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "bin": {
    "edit-ai": "./scripts/cli.js"
  },
  "files": [
    "scripts/",
    "styles/",
    "web-interface/",
    "templates/",
    "config/",
    "README.md",
    "INSTALLATION_GUIDE.md"
  ],
  "directories": {
    "lib": "scripts",
    "test": "tests",
    "docs": "docs"
  },
  "config": {
    "premierePath": {
      "darwin": "/Applications/Adobe Premiere Pro 2024/Adobe Premiere Pro 2024.app/Contents/MacOS/Adobe Premiere Pro 2024",
      "win32": "C:\\Program Files\\Adobe\\Adobe Premiere Pro 2024\\Adobe Premiere Pro.exe"
    },
    "supportedFormats": [
      ".mp4",
      ".mov",
      ".avi",
      ".m4v",
      ".mkv",
      ".wav",
      ".mp3",
      ".aac",
      ".m4a"
    ],
    "defaultStyles": [
      "cinematic",
      "vlog",
      "corporate",
      "music-video",
      "documentary",
      "social-media"
    ],
    "multiVersion": {
      "enabled": true,
      "maxVersions": 6,
      "parallelProcessing": true
    }
  },
  "preferGlobal": true,
  "publishConfig": {
    "access": "public"
  }
}
